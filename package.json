  "license": "UNLICENSED",
  "framework7": {
    "cwd": "/home/aow/workspace/tmpLink",
    "type": [
      "capacitor"
    ],
    "name": "test badge",
    "framework": "core",
    "template": "single-view",
    "bundler": "webpack",
    "cssPreProcessor": "less",
    "theming": {
      "customColor": true,
      "color": "#007aff",
      "darkTheme": false,
      "iconFonts": true,
      "fillBars": true
    },
    "customBuild": true,
    "webpack": {
      "developmentSourceMap": true,
      "productionSourceMap": true,
      "hashAssets": false,
      "preserveAssetsPaths": false,
      "inlineAssets": true
    },
    "pkg": "ch.owitec.test.app"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cd app && cross-env NODE_ENV=development PORT=8082 npm run start",
    "dev-admin": "cd admin && cross-env NODE_ENV=development npm run start",
    "server": "cd server && npm run server",
    "build-dev": "cd app && npm run build-dev",
    "build-dev-admin": "cd admin && npm run build-dev && cp ../src/assets/icon/favicon.ico www-admin",
    "build-dev-android": "cd app && npm run build-dev-android",
    "build-dev-ios": "cd app && npm run build-dev-ios",
    "build-android": "cd app && npm run build-prod-android",
    "build-ios": "cd app && npm run build-prod-ios",
    "couch-local": "couchdb/couchDeployLocal.sh",
    "couch-lamp": "couchdb/couchDeployLamp.sh",
    "assets-android": "cordova-res android --skip-config --copy",
    "assets-ios": "cordova-res ios --skip-config --copy",
    "vite-dev": "cross-env NODE_ENV=development vite",
    "vite-dev-admin": "cross-env NODE_ENV=development vite --config vite-admin.config.js",
    "vite-build": "cross-env NODE_ENV=production vite build && npx cap sync",
    "vite-build-admin": "cross-env NODE_ENV=production vite --config vite-admin.config.js build && npx cap sync",
    "vite-preview": "cross-env NODE_ENV=production vite preview",
    "vite-dev-android": "npm run build-dev && npx cap copy android",
    "vite-dev-ios": "cross-env DEVICE=ios npm run build-dev && npx cap copy ios",
    "vite-build-capacitor-ios": "cross-env DEVICE=ios NODE_ENV=production vite build && cp src/assets/icon/favicon.ico www && npx cap copy ios",
    "vite-build-capacitor-android": "cross-env NODE_ENV=production vite build && cp src/assets/icon/favicon.ico www && npx cap copy android",
    "vite-test-android": "npm run vite-build-capacitor-android && npx cap run android",
    "vite-test-ios": "npm run vite-build-capacitor-ios && npx cap run ios",
    "_vite-push-dev": "rm -R www/* && cp -R src/* www && npx cap sync",
    "vite-postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "****": "",
    "*****": "all: ",
    "compile-android": "npm run build-dev-android",
    "compile-ios": "npm run build-dev-ios",
    "compile-admin": "npm run build-dev-admin",
    "compile-app": "npm run compile-android && npm run compile-ios",
    "compile-all": "npm run compile-android && npm run compile-ios && npm run compile-admin",
    "test-android": "cd app && npm run test-android",
    "test-ios": "cd app && npm run test-ios",
    "build-app": "npm run build-android && npm run build-ios",
    "install-android": "cd app && npm run install-android",
    "install-ios": "cd app && npm run install-ios"
  },
